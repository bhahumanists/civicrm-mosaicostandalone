<?xml version="1.0"?>
<extension key="mosaico-standalone" type="module">
  <file>mosaico_standalone</file>
  <name>Mosaico standalone editor</name>
  <description>Allows you to use the mosaico editor without having access to CiviCRM</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>Matthew Wire</author>
    <email>mjw@mjwconsult.co.uk</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/mjwconsult/civicrm-mosaicostandalone</url>
    <url desc="Support">https://mjw.pt/mosaicostandalone</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2020-08-08</releaseDate>
  <version>0.1</version>
  <develStage>alpha</develStage>
  <compatibility>
    <ver>5.28</ver>
  </compatibility>
  <classloader>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <civix>
    <namespace>CRM/MosaicoStandalone</namespace>
  </civix>
  <requires>
    <ext>uk.co.vedaconsulting.mosaico</ext>
  </requires>
</extension>
